{
  "name": "workadventure-minimapper",
  "version": "1.0.0",
  "description": "an addon to workadveture for a simple minimap button",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaimeXLP/workadventure-minimapper.git"
  },
  "keywords": [
    "workadventure"
  ],
  "author": "Leon Prinz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RaimeXLP/workadventure-minimapper/issues"
  },
  "homepage": "https://github.com/RaimeXLP/workadventure-minimapper#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.6",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.2"
  }
}
